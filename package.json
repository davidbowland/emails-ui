{
  "name": "emails-ui",
  "version": "1.15.0",
  "description": "UI for emails",
  "author": "David Bowland",
  "engines": {
    "node": "^20.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/emails-ui.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/emails-ui/issues"
  },
  "homepage": "https://github.com/davidbowland/emails-ui#readme",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf .cache coverage public && npm ci && gatsby clean",
    "deploy": "DEVELOPMENT=true npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "start": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "npx update-browserslist-db@latest && ncu --doctor --target minor --upgrade && npm audit fix --audit-level=none && npm run test && npm dedupe"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.6.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^4.5.8",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.18.0",
    "@mui/lab": "^5.0.0-alpha.177",
    "@mui/material": "^5.18.0",
    "@mui/x-data-grid": "^6.20.4",
    "aws-amplify": "^5.3.29",
    "axios": "^1.13.2",
    "babel-plugin-styled-components": "^2.1.4",
    "crypto-browserify": "^3.12.1",
    "dompurify": "^3.3.1",
    "fast-json-patch": "^3.1.1",
    "gatsby": "^5.15.0",
    "gatsby-legacy-polyfills": "^3.15.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-mdx": "^5.15.0",
    "gatsby-plugin-sass": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-styled-components": "^6.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "graphql": "^16.12.0",
    "husky": "^9.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.15.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-jest": "^28.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "prettier": "^3.7.4",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.4.6",
    "undici": "^6.23.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
